{
  "manifest_version": 3,
  "name": "URL Lockup",
  "version": "0.5.0",
  "description": "Prevent phishing with urlLockup",
  "author": "Victor Gon√ßalves da Luz",

  "permissions": [
    "notifications",
    "tabs"
  ],

  "action": {
    "default_icon": {
      "16": "img/icon16.png",
      "24": "img/icon24.png",
      "32": "img/icon32.png",
      "64": "img/icon64.png"
    },
    "default_title": "URL Lockup",
    "default_popup": "index.html"
  },

  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "128": "img/icon128.png"
  },

  "background": {
    "service_worker": "background.js"
  }
}